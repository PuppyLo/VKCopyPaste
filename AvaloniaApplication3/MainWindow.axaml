<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="540" d:DesignHeight="960"
        x:Class="AvaloniaApplication3.MainWindow"
        Title="AvaloniaApplication3">
  
  <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Vertical">
    <TextBlock Text="{Binding GroupName}" TextAlignment="Center" Margin="25"> </TextBlock>
    <Button Content="Start" Click="StartButton" HorizontalAlignment="Center"></Button>
    <Image Source="{Binding GroupAva}" Width="150" Height="150" HorizontalAlignment="Center"></Image>
  </StackPanel>
</Window>
